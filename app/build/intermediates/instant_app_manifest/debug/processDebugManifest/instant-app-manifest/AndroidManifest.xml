<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="profoundmasteryinidiocy.gmail.com.schedulermaybe"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="28" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="android.support.v4.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@drawable/fblareadylogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/fblareadylogo"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme.NoActionBar" >
        <activity
            android:name="profoundmasteryinidiocy.gmail.com.schedulermaybe.Activities.home"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="profoundmasteryinidiocy.gmail.com.schedulermaybe.Activities.Register"
            android:screenOrientation="portrait" />
        <activity
            android:name="profoundmasteryinidiocy.gmail.com.schedulermaybe.Activities.categorySelection"
            android:label="@string/title_activity_test"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name="profoundmasteryinidiocy.gmail.com.schedulermaybe.Activities.MainActivity"
            android:label="@string/title_activity_home"
            android:screenOrientation="portrait" />
        <activity
            android:name="profoundmasteryinidiocy.gmail.com.schedulermaybe.Activities.TopicSelection"
            android:label="@string/title_activity_topic_selection"
            android:screenOrientation="portrait" />
        <activity
            android:name="profoundmasteryinidiocy.gmail.com.schedulermaybe.Activities.QuizActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="profoundmasteryinidiocy.gmail.com.schedulermaybe.Activities.QuizResults"
            android:screenOrientation="portrait" />
        <activity
            android:name="profoundmasteryinidiocy.gmail.com.schedulermaybe.Activities.ResultDetails"
            android:screenOrientation="portrait" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="profoundmasteryinidiocy.gmail.com.schedulermaybe.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>